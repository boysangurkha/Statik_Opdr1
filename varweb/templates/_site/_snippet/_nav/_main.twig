{% set mainNav = craft.navigate.raw('mainNav') %}
{% set searchResultsEntry = craft.entries.section('searchResults').one() %}

{% if mainNav|length %}
	<nav class="navigatie w-full flex items-center justify-end h-full relative " aria-label="{{ 'Main'|t }}">


		
		<ul class="flex flex-wrap space-x-5 items-center text-pitchblack" >
			{% for node in mainNav %}
				<li class="xl:py-6 xxs:py-8 {% if node.classes|length %}{{ node.classes }}{% endif %}">
					<a href="{{ node.url }}" class="font-sans text-md hover:no-underline {% if node.active %}active{% endif %}" {% if node.blank %} target="_blank" rel="noopener" {% endif %}>{{ node.name }}
					</a>
				</li>
			{% endfor %}
			{# {% if searchResultsEntry %} #}
		{#                <li>#}
			{#                    {% include '_site/_snippet/_global/_search' %}#}
			{#                </li>#}
			{# {% endif %} #}
		</ul>
		
	</nav>
{% endif %}
